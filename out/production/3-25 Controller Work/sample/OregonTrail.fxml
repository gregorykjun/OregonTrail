                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <children>
                            <ImageView fx:id="startmenubackground" fitHeight="389.0" fitWidth="750.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../resources/Oregon Trail.jpg" />
                                </image>
                            </ImageView>
                            <Button fx:id="startMenuBtn" layoutX="351.0" layoutY="192.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="48.0" text="Start" />
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
          <Tab text="Untitled Tab 2">
              <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
          </Tab>
          <Tab text="Untitled Tab 3">
            <content>
                <StackPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="400.0" prefWidth="1500.0">
                            <children>
                                <ImageView fx:id="background2" fitHeight="568.0" fitWidth="761.0" layoutX="750.0" layoutY="-160.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../resources/loopingbackground.PNG" />
                                    </image>
                                </ImageView>
                                <ImageView fx:id="background1" fitHeight="587.0" fitWidth="781.0" layoutX="-15.0" layoutY="-176.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../resources/loopingbackground.PNG" />
                                    </image>
                                </ImageView>
                                <ImageView fitHeight="107.0" fitWidth="88.0" layoutX="666.0" layoutY="9.0" onMouseClicked="#handleItems" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../resources/Backpack.png" />
                                    </image>
                                </ImageView>
                                <Button fx:id="window" layoutY="2.0" mnemonicParsing="false" text="Button" visible="false" />
