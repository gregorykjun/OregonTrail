                                <TextField fx:id="WName" layoutX="297.0" layoutY="228.0">
                                    <font>
                                        <Font name="Courier New" size="12.0" />
                                    </font>
                                </TextField>
                                <TextField fx:id="MCName" layoutX="297.0" layoutY="181.0">
                                    <font>
                                        <Font name="Courier New" size="12.0" />
                                    </font>
                                </TextField>
                                <Label layoutX="188.0" layoutY="228.0" text="Wife Name:" textFill="WHITE">
                                    <font>
                                        <Font name="Courier New" size="18.0" />
                                    </font>
                                </Label>
                                <Label fx:id="C1lbl" layoutX="156.0" layoutY="276.0" text="Child 1 Name:" textFill="WHITE">
                                    <font>
                                        <Font name="Courier New" size="18.0" />
                                    </font>
                                </Label>
                                <Label fx:id="C2lbl" layoutX="156.0" layoutY="323.0" text="Child 2 Name:" textFill="WHITE">
                                    <font>
                                        <Font name="Courier New" size="18.0" />
                                    </font>
                                </Label>
                                <Ellipse fill="#eaf2c7" layoutX="347.0" layoutY="131.0" radiusX="42.0" radiusY="20.0" stroke="BLACK" strokeType="INSIDE" />
                        <ImageView fx:id="MCImage" fitHeight="88.0" fitWidth="76.0" layoutX="310.0" layoutY="63.0" pickOnBounds="true" preserveRatio="true" />
                        <Button fx:id="startGameBtn" layoutX="303.0" layoutY="364.0" mnemonicParsing="false" onAction="#startGame" text="Start Game">
                           <font>
                              <Font name="Courier New" size="12.0" />
                           </font>
                        </Button>
                            </children></AnchorPane>
                    </content>
                </Tab>
                <Tab text="Untitled Tab 4">
                    <content>
                        <StackPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Pane prefHeight="400.0" prefWidth="1500.0">
                                    <children>
                                        <ImageView fx:id="background2" fitHeight="568.0" fitWidth="761.0" layoutX="750.0" layoutY="-160.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../resources/loopingbackground.PNG" />
                                            </image>
                                        </ImageView>
                                        <ImageView fx:id="background1" fitHeight="587.0" fitWidth="781.0" layoutX="-15.0" layoutY="-176.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../resources/loopingbackground.PNG" />
                                            </image>
                                        </ImageView>
                                        <ImageView fitHeight="107.0" fitWidth="88.0" layoutX="666.0" layoutY="9.0" onMouseClicked="#handleItems" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../resources/Backpack.png" />
                                            </image>
                                        </ImageView>
                                    </children>
                                </Pane>
                            </children>
                        </StackPane>

                    </content>
                </Tab>
                <Tab text="Untitled Tab 5">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab text="Untitled Tab 6">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
