                                <Label layoutX="255.0" layoutY="25.0" text="Choose A Character." textFill="WHITE">
                                    <font>
                                        <Font name="Courier New Bold" size="21.0" />
                                    </font>
                                </Label>
                                <TextArea fx:id="characterDescription" editable="false" layoutX="232.0" layoutY="213.0" prefHeight="154.0" prefWidth="287.0" style="-fx-text-inner-color: white; -fx-control-inner-background: black;" visible="false" wrapText="true">
                                    <font>
                                        <Font name="Courier New" size="14.0" />
                                    </font>
                                </TextArea>
                        <ImageView fx:id="normalModeImage" fitHeight="107.0" fitWidth="73.0" layoutX="339.0" layoutY="108.0" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="hardModeImage" fitHeight="103.0" fitWidth="84.0" layoutX="424.0" layoutY="79.0" pickOnBounds="true" preserveRatio="true" />
                        <ImageView fx:id="easyModeImage" fitHeight="111.0" fitWidth="81.0" layoutX="245.0" layoutY="81.0" pickOnBounds="true" preserveRatio="true" />
                                <Circle fill="#1f93ff00" layoutX="285.0" layoutY="118.0" onMouseClicked="#easyModeClicked" onMouseEntered="#easyDescription" onMouseExited="#clearDescription" opacity="0.0" radius="43.0" stroke="BLACK" strokeType="INSIDE" />
                        <Circle fill="#1f93ff00" layoutX="375.0" layoutY="152.0" onMouseClicked="#normalModeClicked" onMouseEntered="#normalDescription" onMouseExited="#clearDescription" opacity="0.0" radius="43.0" stroke="BLACK" strokeType="INSIDE" />
                        <Circle fill="#1f93ff00" layoutX="467.0" layoutY="118.0" onMouseClicked="#hardModeClicked" onMouseEntered="#hardDescription" onMouseExited="#clearDescription" opacity="0.0" radius="43.0" stroke="BLACK" strokeType="INSIDE" />
