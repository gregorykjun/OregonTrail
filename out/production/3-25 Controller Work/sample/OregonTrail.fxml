                           <font>
                              <Font name="Courier New" size="15.0" />
                           </font>
                        </Label>
                        <Button fx:id="confirmBtn" layoutX="607.0" layoutY="257.0" mnemonicParsing="false" onAction="#confirmSelectionCharacter" text="Confirm" visible="false">
                           <font>
                              <Font name="Courier New" size="14.0" />
                           </font>
                        </Button>
                            </children></AnchorPane>
                    </content>
                </Tab>
                <Tab text="Untitled Tab 3">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Label layoutX="242.0" layoutY="181.0" text="Name:" textFill="WHITE">
                                    <font>
                                        <Font name="Courier New" size="18.0" />
                                    </font></Label>
                                <TextField fx:id="C1Name" layoutX="297.0" layoutY="276.0">
                                    <font>
                                        <Font name="Courier New" size="12.0" />
                                    </font></TextField>
                                <TextField fx:id="C2Name" layoutX="297.0" layoutY="323.0">
                                    <font>
                                        <Font name="Courier New" size="12.0" />
                                    </font>
