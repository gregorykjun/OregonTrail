<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="1500.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <TabPane fx:id="tabPane" layoutY="-35.0" prefHeight="438.0" prefWidth="750.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Untitled Tab 1">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ImageView fx:id="startmenubackground" fitHeight="389.0" fitWidth="750.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../resources/Oregon Trail.jpg" />
                                    </image>
                                </ImageView>
                                <Button fx:id="startMenuBtn" layoutX="351.0" layoutY="192.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="48.0" text="Start" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Untitled Tab 2">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Ellipse fill="#eaf2c7" layoutX="375.0" layoutY="178.0" radiusX="40.0" radiusY="17.0" stroke="BLACK" strokeType="INSIDE" />
                                <Ellipse fill="#eaf2c7" layoutX="466.0" layoutY="144.0" radiusX="40.0" radiusY="17.0" stroke="BLACK" strokeType="INSIDE" />
